doctype html
html.no-js(lang='en')
    head
        meta(charset='UTF-8')
        meta(http-equiv='X-UA-Compatible', content='IE=edge')
        meta(name='viewport', content='width=device-width, initial-scale=1')
        title DIG 2500 &mdash; Prototype

        link(href='https://fonts.googleapis.com/css?family=Roboto+Condensed|Abril+Fatface', rel='stylesheet')
        link(rel='stylesheet', type='text/css', href='assets/styles/style.css')
        //if IE
            script(src='http://html5shiv.googlecode.com/svn/trunk/html5.js')
        script.
            document.documentElement.className = 'js';
    body
      main
        .browser-alert
          a(href="https://www.google.com/chrome/browser/desktop/index.html" target="_blank") For best viewing results use Google Chome.
        section#board.content.content--modifier
            span.section-title
              span.green 'Gotchas' in
              |  Web Development
            .slideshow.slideshow--modifier(data-effect='spring')
                .slide.slide--current
                    h2.title.title--centered.title--modifier Slide #1
                .slide
                    h2.title.title--centered.title--modifier Slide #2
                .slide
                    h2.title.title--centered.title--modifier Slide #3
                .slide
                    h2.title.title--centered.title--modifier Slide #4
                .slide
                    h2.title.title--centered.title--modifier Slide #5
                .slide
                    h2.title.title--centered.title--modifier Slide #6
            nav.actions
                button.btn.btn--trigger.btn--prev Previous
                button.btn.btn--trigger.btn--next Next
//.footer-block
script(src='assets/js/charming.min.js')
